{
	"name": "redactor-test",
	"version": "1.0.0",
	"description": "A minimal Electron application",
	"main": "main.js",
	"scripts": {
		"prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
		"start": "webpack-dev-server --hot --host 0.0.0.0 --port 9292 --config=./webpack.dev.config.js --mode development",
		"build": "webpack --config webpack.build.config.js --mode production",
		"package": "npm run build && electron-packager ./ --out=./builds --platform=all",
		"dist": "build",
		"dist-x": "build -ml",
		"release": "build"
	},
	"repository": "https://github.com/electron/electron-quick-start",
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo"
	],
	"build": {
		"appId": "com.theguardian.cv-redactor"
	},
	"author": "GitHub",
	"license": "CC0-1.0",
	"dependencies": {
		"convert-array-to-csv": "^1.0.4",
		"hummus": "^1.0.90",
		"lodash": "^4.17.11",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-redux": "^5.0.7",
		"redux": "^4.0.1",
		"redux-devtools-extension": "^2.13.5",
		"redux-multi": "^0.1.12",
		"redux-thunk": "^2.3.0",
		"remove-accents": "^0.4.2",
		"slugify": "^1.3.3",
		"uniqid": "^5.0.3"
	},
	"devDependencies": {
		"electron": "^1.8.6",
		"electron-builder": "^20.38.3",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-react": "^6.24.1",
		"babili-webpack-plugin": "^0.1.2",
		"css-loader": "^0.28.11",
		"electron-devtools-installer": "^2.2.4",
		"electron-packager": "^12.0.1",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.0",
		"postcss-cssnext": "^3.1.0",
		"postcss-import": "^11.1.0",
		"postcss-loader": "^2.1.4",
		"postcss-nested": "^3.0.0",
		"postcss-pxtorem": "^4.0.1",
		"style-loader": "^0.21.0",
		"webpack": "^4.6.0",
		"webpack-cli": "^2.0.15",
		"webpack-dev-server": "^3.1.3"
	}
}
